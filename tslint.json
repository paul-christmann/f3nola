{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "comment-format": false,
    "interface-name": false,
    "max-classes-per-file": [true, 5],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}
